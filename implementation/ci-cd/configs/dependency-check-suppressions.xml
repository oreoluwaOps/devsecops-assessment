<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Spring Boot framework dependencies -->
    <suppress>
        <notes><![CDATA[
        Spring Boot Actuator - Low risk in development environment with proper security configuration
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-actuator.*$</packageUrl>
        <cve>CVE-2021-22060</cve>
    </suppress>
    
    <!-- Suppress low-severity vulnerabilities in test dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not part of production runtime
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/.*$</packageUrl>
        <cve regex="true">CVE-.*</cve>
        <cvssBelow>4.0</cvssBelow>
    </suppress>
    
    <!-- Suppress vulnerabilities in specific versions that don't apply to our usage -->
    <suppress>
        <notes><![CDATA[
        Jackson databind - Vulnerability doesn't apply to our specific usage pattern
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson-databind.*$</packageUrl>
        <cve>CVE-2020-36518</cve>
    </suppress>
    
    <!-- Suppress vulnerabilities in embedded Tomcat when using Spring Boot -->
    <suppress>
        <notes><![CDATA[
        Embedded Tomcat in Spring Boot with proper configuration is not vulnerable
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.tomcat\.embed/.*$</packageUrl>
        <cve regex="true">CVE-2021-30640</cve>
    </suppress>
    
    <!-- Suppress vulnerabilities in development-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Development and build-time dependencies
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-devtools.*$</packageUrl>
        <cve regex="true">CVE-.*</cve>
    </suppress>
    
    <!-- Example suppression for false positive CPE matching -->
    <suppress>
        <notes><![CDATA[
        False positive CPE match - this is not the vulnerable component
        ]]></notes>
        <cpe>cpe:/a:example:vulnerable_component</cpe>
        <cve>CVE-2021-12345</cve>
    </suppress>
    
</suppressions>
